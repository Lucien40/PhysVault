@article{herrmannGravitationalBremsstrahlungReverse2021,
  title = {Gravitational {{Bremsstrahlung}} from {{Reverse Unitarity}}},
  author = {Herrmann, Enrico and {Parra-Martinez}, Julio and Ruf, Michael S. and Zeng, Mao},
  year = {2021},
  month = may,
  journal = {Physical Review Letters},
