  langid = {english},
  file = {D\:\\DOC\\UNI\\ETH\\READ\\zotero-lib\\Book\\Lee\\2012\\Lee_2012_Introduction to Smooth Manifolds.pdf;D\:\\DOC\\UNI\\ETH\\READ\\zotero-lib\\Book\\Lee\\2012\\Lee_2012_Introduction to Smooth Manifolds.html}
