
@article{abreuCutsResiduesOneloop2017,
  title = {Cuts from Residues: The One-Loop Case},
  shorttitle = {Cuts from Residues},
  author = {Abreu, Samuel and Britto, Ruth and Duhr, Claude and Gardi, Einan},
  year = {2017},
